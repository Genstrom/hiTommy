@model dynamic
@{
    ViewData["Title"] = "_MyMenuLayout";
}

<nav>


    <div class="box box-navbar">
        <ul class="nav-items">
            <li>
                <a href="/" Home">All</a>
            </li>
            @for (var i = 0; i < Model.Brand.Count; i++)
            {
                <li>
                    <a href="/Brand/@Model.Brand[i].Id/">@Model.Brand[i].Name</a>
                </li>
            }
        </ul>
    </div>
</nav>